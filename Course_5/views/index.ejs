<body>

    <%- include('./partials/nav.ejs') %> 

    </nav>
    <div class="blogs content">
        <h2>All Blogs</h2>
        <% if(blogs.length > 0) { %>
            <% blogs.forEach(blog => { %> 
                <h3 class="title"><%= blog.judul %> </h3> 
                <p class="snippet"><%= blog.snippet %> </p>
            <% }) %> 
        <% } else {%> 
                <p>Maaf, tidak ada data disini !</p>
            <% } %> 
    </div>

      <%- include('./partials/footer.ejs') %> 

</body>
</html>